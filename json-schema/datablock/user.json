{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "User Datablock",
	"$id": "af.datablock.user",
	"description": "This datablock describes the current user.",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"display_name": {
			"type": "string"
		},
		"display_tier": {
			"type":"string"
		},
        "display_icon_uri":{
            "type":"string",
            "format": "uri"
        }
	},
	"required": [
		"title"
	]
}