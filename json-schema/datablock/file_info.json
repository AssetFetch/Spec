{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "File Info Datablock",
	"$id": "af.datablock.file_info",
	"description": "Datablock for general information about a component file.",
	"type": "object",
	"properties": {
		"length": {
			"type": "integer",
			"minimum": 0
		},
		"extension": {
			"type": "string",
			"pattern": "(\\.[a-z0-9]+)+"
		}
	},
	"required": [
		"extension"
	]
}