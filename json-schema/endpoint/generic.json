{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"meta": {
			"$ref": "/template/meta.json"
		},
		"data": {
			"type": "object",
			"patternProperties": {
				"^[a-z0-9_]+(\\.[a-z0-9_]+)?$": {
					"oneOf": [
						{
							"type": "object"
						},
						{
							"type": "array"
						}
					]
				}
			},
			"additionalProperties": false
		}
	},
	"required": [
		"meta",
		"data"
	]
}