{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"meta": {
			"$ref": "/template/meta.json"
		},
		"data": {
			"oneOf": [
				{
					"type": "object",
					"patternProperties": {
						"^[a-z0-9_]+(\\.[a-z0-9_]+)?$": {
							"type": "object"
						}
					},
					"additionalProperties": false
				},
				{
					"type": "array"
				}
			]
		}
	},
	"required": [
		"meta",
		"data"
	]
}