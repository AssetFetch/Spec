{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Fixed Query Template",
	"$id":"/template/fixed_query.json",
	"type": "object",
	"properties": {
		"uri": {
			"type": "string"
		},
		"method": {
			"type": "string",
			"enum": ["GET", "POST"]
		},
		"payload": {
			"type": "object",
			"additionalProperties": {
				"type": "string"
			}
		}
	},
	"required": [
		"uri",
		"method",
		"payload"
	]
}
