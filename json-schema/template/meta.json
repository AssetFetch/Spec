{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "Template: Meta",
	"$id": "af.template.meta",
	"description": "This template contains metadata for every endpoint.",
	"type": "object",
	"properties": {
		"response_id": {
			"type": "string"
		},
		"version": {
			"type": "string"
		},
		"kind": {
			"type": "string"
		},
		"message": {
			"type": "string"
		}
	},
	"required": [
		"kind",
		"version"
	]
}