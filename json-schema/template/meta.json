{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "Template: Meta",
	"$id": "af.template.meta",
	"description": "This template contains metadata for every endpoint.",
	"type": "object",
	"properties": {
		"response_id": {
			"type": [
				"string",
				"null"
			]
		},
		"version": {
			"type": "string",
			"pattern": "[0-9]\\.[0-9]"
		},
		"kind": {
			"type": "string"
		},
		"message": {
			"type": [
				"string",
				"null"
			]
		},
		"required": [
			"kind",
			"version"
		]
	}
}